{
  "name": "vibe-coder-3d",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonit-dev/vibe-coder-3d.git"
  },
  "bugs": {
    "url": "https://github.com/jonit-dev/vibe-coder-3d/issues"
  },
  "homepage": "https://github.com/jonit-dev/vibe-coder-3d#readme",
  "keywords": [
    "game-engine",
    "3d",
    "react-three-fiber",
    "rust",
    "ecs",
    "physics",
    "ai-first",
    "webgl",
    "three.js"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "node scripts/sync-assets.js --silent && node scripts/optimize.js --silent & vite",
    "dev:nosync": "vite",
    "dev:sync": "node scripts/sync-assets.js",
    "optimize": "node scripts/optimize.js",
    "optimize:force": "node scripts/optimize.js --force",
    "build": "node scripts/sync-assets.js --once --silent && node scripts/optimize.js --silent && tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --silent",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:file": "vitest run --reporter=verbose",
    "typecheck": "tsc && yarn lint",
    "verify": "yarn tsc && yarn lint:fix && yarn test",
    "verify:scene": "tsx scripts/validate-scene.ts",
    "validate:scene": "node scripts/validate-scene.cjs",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepare": "husky",
    "transpile:lua": "tstl -p tsconfig.tstl.json",
    "transpile:lua:watch": "tstl -p tsconfig.tstl.json --watch",
    "rust:export-schemas": "node scripts/export-schemas.js",
    "rust:sync-assets": "yarn transpile:lua && node scripts/sync-assets-to-rust.js",
    "rust:engine": "yarn rust:sync-assets --silent && cd rust/engine && cargo run --",
    "rust:engine:test": "yarn rust:sync-assets --silent && cd rust/engine && cargo run",
    "rust:engine:verbose": "yarn rust:sync-assets --silent && cd rust/engine && cargo run -- --verbose",
    "rust:screenshot": "yarn rust:sync-assets --silent && cd rust/engine && cargo run -- --scene testphysics --screenshot",
    "rust:test": "cargo test --manifest-path rust/engine/Cargo.toml",
    "rust:test:watch": "cargo watch -x 'test --manifest-path rust/engine/Cargo.toml'",
    "rust:build": "cargo build --manifest-path rust/engine/Cargo.toml",
    "rust:build:release": "cargo build --release --manifest-path rust/engine/Cargo.toml",
    "rust:check": "cargo check --manifest-path rust/engine/Cargo.toml",
    "rust:clean": "cargo clean --manifest-path rust/engine/Cargo.toml",
    "render:geometry": "node scripts/render-geometry.js",
    "threejs:screenshot": "node scripts/capture-threejs-screenshot.mjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gltf-transform/cli": "^4.1.3",
    "@gltf-transform/core": "^4.1.3",
    "@gltf-transform/extensions": "^4.1.3",
    "@gltf-transform/functions": "^4.1.3",
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/rapier": "^2.1.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/howler": "^2.2.12",
    "@types/styled-components": "^5.1.34",
    "adm-zip": "^0.5.16",
    "bitecs": "^0.3.40",
    "daisyui": "^5.0.28",
    "dotenv": "^17.2.3",
    "draco3dgltf": "^1.5.7",
    "howler": "^2.2.4",
    "meshoptimizer": "^0.23.0",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^2.1.8",
    "react-router-dom": "^7.5.2",
    "styled-components": "^6.1.17",
    "three": "^0.175.0",
    "three-mesh-bvh": "^0.9.1",
    "uuid": "^13.0.0",
    "zod": "^3.24.3",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/adm-zip": "^0.5.7",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.175.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "3.2.3",
    "@vitest/ui": "^3.2.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "puppeteer": "^24.25.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "typescript-to-lua": "^1.32.0",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
